{
  "name": "projectos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "npx tsc --noEmit",
    "health-check": "node scripts/health-check.js",
    "quality-gate": "npm run health-check && npm run lint && npm run type-check && npm run build",
    "pre-deploy": "npm run quality-gate",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed",
    "db:studio": "npx prisma studio",
    "vercel-build": "npm run db:generate && npm run build",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@prisma/client": "^6.11.1",
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.3",
    "next": "15.4.2",
    "next-i18next": "^15.4.2",
    "next-themes": "^0.4.6",
    "prisma": "^6.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}
